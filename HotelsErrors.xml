<?xml version="1.0" encoding="utf-8"?>
<!-- 1) The root is <Hotel> instead of <Hotels> -->
<Hotel>
  
  <!-- 2) Duplicate <Name> elements -->
  <Hotel>
    <Name>Westin Tempe</Name>
    <Name>DuplicateName</Name>
    <!-- 3) Missing <Phone> entirely -->
    <Address NearestAirport="Sky Harbor">
      <Number>11</Number>
      <Street>E 7th St</Street>
      <City>Tempe</City>
      <State>AZ</State>
      <Zip>85281</Zip>
    </Address>
    <Rating>4.2</Rating>
  </Hotel>

  <!-- Another hotel with more brokenness, e.g. unclosed <Address> -->
  <Hotel>
    <Name>Marriott Downtown</Name>
    <Phone>888-555-1212</Phone>
    <!-- Suppose your XSD says NearestAirport is required but we skip it or break it -->
    <Address>
      <Number>100</Number>
      <Street>Main Street
      <!-- 4) We do not close <Street> or <Address> properly -->
      <City>Phoenix</City>
      <State>AZ</State>
      <Zip>85001</Zip>
    </Address>
  </Hotel>

</Hotel>
